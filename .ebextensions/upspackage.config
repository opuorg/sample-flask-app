packages:
  yum:
    git: []
    httpd: []

groups:
  upsgroup: []

users:
  upsuser:
    groups:
      - upsgroup
    uid: 50
    homeDir: /home/upsuser

files:
  "/home/upsuser/hello.txt":
    mode: "000644"
    owner: upsuser
    group: upsgroup
    content: |
      this is my file content

commands:
  command1:
    command: "mkdir -p /var/app/logs"
  command2:
    command: "echo 'Hello I created this' > /var/app/logs/deploy.log"

services:
  sysvinit:
    httpd:
      enabled: true
      ensureRunning: true


