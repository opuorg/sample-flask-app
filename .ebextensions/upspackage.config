packages:
  yum:
    git: []
groups:
  upsgroup: []
users:
  upsuser:
    groups:
      - upsgroup
      uid: "50"
      homeDir: /home/upsuser
sources:
  /etc/upsuser: https://amzn-s3-demo-bucket.s3.amazonaws.com/myobject

files:
  "/home/upsuser/hello.txt":
    mode: "000644"
    owner: upsuser
    group: upsgroup
    content: |
    this is my file content
commands:
  command1:
    command: "mkdir -p /var/app/logs"
  command2:
    command: "echo 'Hello I created this' > /var/app/logs/deploy.log"
